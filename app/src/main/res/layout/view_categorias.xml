<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="2dp"
    android:layout_marginTop="3dp"
    android:layout_marginRight="2dp"
    android:layout_marginBottom="3dp">

    <LinearLayout
        android:id="@+id/itemadapt"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/pictogramadapter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/categlist2"
            tools:layout_conversion_wrapHeight="126"
            tools:layout_conversion_wrapWidth="126" />

        <TextView
            android:id="@+id/textodapter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="TextView" />

        <LinearLayout
            android:id="@+id/barrview"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#24CDB8"
            android:orientation="horizontal"
            android:visibility="invisible"></LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
